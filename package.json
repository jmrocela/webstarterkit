{
  "name": "webclient",
  "version": "2.0.0",
  "description": "See README.md",
  "contributors": [
    "John Rocela <jamoy@hooq.tv>"
  ],
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "NODE_ENV=development concurrently --kill-others \"nodemon webclient.js\" \"npm run watch\" \"gulp watch\"",
    "build": "NODE_ENV=production webpack --config webpack/webpack.config.js && webpack --config webpack/webpack.prod.js",
    "watch": "webpack-dev-server --config webpack/webpack.server.js",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "private": true,
  "jest": {
    "setupFiles": ["./jestsetup.js"],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/resources/*",
      "/webpack/*",
      "/lib/*"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.7.19",
    "bluebird": "^3.4.7",
    "bootstrap": "^4.0.0-alpha.6",
    "dotenv": "^4.0.0",
    "dustjs-helpers": "^1.7.3",
    "dustjs-linkedin": "^2.7.5",
    "glob": "^7.1.1",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.2.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^3.0.4",
    "koa-generic-session": "^1.11.3",
    "koa-helmet": "^2.0.0",
    "koa-redis": "^2.1.2",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "koa-views": "^5.2.0",
    "mobile-detect": "^1.3.5",
    "moment": "^2.17.1",
    "newrelic": "^1.36.0",
    "nprogress": "^0.2.0",
    "react": "^15.4.2",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.4.2",
    "react-helmet": "^3.3.0",
    "react-intl": "^2.2.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redis": "^2.6.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0",
    "uuid": "^3.0.1",
    "winston": "^2.3.0",
    "winston-elasticsearch": "^0.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "concurrently": "^3.1.0",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.2",
    "gulp-ext-replace": "^0.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-uglify": "^2.0.0",
    "gulp-wrap": "^0.13.0",
    "jest": "^18.1.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-node-externals": "^1.5.4"
  }
}
